{
  "name": "full-stack-react",
  "version": "1.0.0",
  "type": "module",
  "workspaces": [
    "server",
    "client"
  ],
  "scripts": {
    "dev": "concurrently \"npm:dev:*\"",
    "dev:client": "npm run dev --workspace=client",
    "dev:server": "npm run dev --workspace=server",
    "db:seed": "psql -f server/migration.sql tasktime"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.1.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4"
  },
  "dependencies": {
    "react-icons": "^4.7.1"
  }
}
